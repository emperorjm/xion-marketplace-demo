{
  "name": "marketplace-console",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "UI for interacting with asset and marketplace CosmWasm contracts",
  "license": "Apache-2.0",
  "scripts": {
    "generate-env": "node scripts/generate-env.mjs",
    "predev": "npm run generate-env",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "prebuild": "npm run generate-env",
    "build": "tsc -b && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist-server/index.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@burnt-labs/abstraxion": "^1.0.0-alpha.69",
    "@cosmjs/cosmwasm-stargate": "^0.37.0",
    "@cosmjs/encoding": "^0.37.0",
    "@cosmjs/proto-signing": "^0.37.0",
    "@cosmjs/stargate": "^0.37.0",
    "@noble/hashes": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "graz": "^0.4.2",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.4.2",
    "vite": "^5.4.11",
    "vite-plugin-node-polyfills": "^0.24.0"
  }
}
