{
  "name": "marketplace-console",
  "version": "0.1.0",
  "description": "Nest-powered UI scaffold for interacting with asset and marketplace CosmWasm contracts",
  "main": "dist/main.js",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run client:build && nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "client:dev": "npm --prefix client run dev",
    "client:build": "npm --prefix client run build"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.7",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/serve-static": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.3.7",
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
